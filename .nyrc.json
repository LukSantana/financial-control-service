{
  "extension": [".ts", ".tsx", ".js"],
  "require": ["ts-node/register"],
  "include": ["src/**/*.ts"],
  "exclude": ["tests/**/*.ts", "node_modules", "dist", "coverage", "**/*.d.ts"],
  "reporter": ["text", "html"],
  "check-coverage": true,
  "all": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
